<?php
function work_hard($nr) {
    $nr = preg_replace('/\D/', '', $nr);
    $nr_length = strlen($nr);
    $prty = $nr_length % 2;
    $ttl = 0;
    for ($i = 0;$i < $nr_length;$i++) {
        $dgt = $nr[$i];
        if ($i % 2 == $prty) {
            $dgt*= 2;
            if ($dgt > 9) {
                $dgt-= 9;
            }
        }
        $ttl+= $dgt;
    }
    if ($ttl % 10 == 0) {
        return "valid";
    } else return "invalid";
}
